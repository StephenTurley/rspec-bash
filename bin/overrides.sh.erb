#!/usr/bin/env bash
requote() {
  local res=""
  for x in "${@}" ; do
    grep -q "[[:space:]]" <<< "$x" && res="${res} '${x}'" || res="${res} ${x}"
  done
  sed -e 's/^ //' <<< "${res}"
}

function <%=command%> {
  local arguments=$(requote "${@}")
  bash -c "<%=command_path%> ${arguments}"
}

readonly -f <%=command%> &> /dev/null
